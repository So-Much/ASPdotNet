{
	// Place your snippets for vue here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	
	// My Snippets for Vue 3 Composition API Reference
	// VueInit
	"VueInit": {
		"prefix": "vueinit",
		"body": [
			"<script setup>",
			"$2",
			"</script>",
			"",
			"<template>",
			"\t$1",
			"</template>",
			"",
			"<style scoped>",
			"$3",
			"</style>"
		],
		"description": "Use this snippet to initialize a variable in Vue 3 Composition API"
	},
	// Init Script Tag
	"Script tag setup in Vue 3": {
		"prefix": "scriptsetup",
		"body": [
			"<script setup>",
			"$1",
			"</script>"
		],
		"description": "Use this snippet to setup script tag in Vue 3"
	},
	// Import ref from Vue
	"Import ref from Vue": {
		"prefix": "ipref",
		"body": [
			"import { ref } from 'vue';"
		],
		"description": "Use this snippet to import ref from Vue"
	},
	//props Initialized
	"Props Initialized": {
		"prefix": "propsinit",
		"body": [
			"import { defineProps, onMounted } from 'vue';",
			"const props = defineProps({",
			// comment this is normal property declaration
			"\/\/ Normal property declarations",
			"\t// $1: $2",
			// comment this is styles property declaration
			"\/\/ Styles property declarations",
			"\t// styles: Object",
			"});",
			"\n",
			"onMounted(() => {",
			"\t// $3",
			"});"
		],
		"description": "Use this snippet to initialize props in Vue 3 Composition API"
	},
}